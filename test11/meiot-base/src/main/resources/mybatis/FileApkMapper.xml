<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.meiot.mapper.FileApkMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.meiot.entity.FileApk">
        <id column="id" property="id" />
        <result column="apk_name" property="apkName" />
        <result column="version_name" property="versionName" />
        <result column="description" property="description" />
        <result column="file_size" property="fileSize" />
        <result column="file_url" property="fileUrl" />
        <result column="is_default" property="isDefault" />
        <result column="create_time" property="createTime" />
        <result column="type" property="type" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, apk_name, version_name, description, file_size, file_url, is_default, create_time, `type`
    </sql>

</mapper>
