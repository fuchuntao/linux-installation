<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.meiot.dao.HuaweiEquipmentMapper">


    <select id="querySerialNumber" resultType="java.lang.String">
        SELECT serial_number FROM `huawei_equipment` WHERE device_id = #{deviceId} LIMIT 1
    </select>
</mapper>